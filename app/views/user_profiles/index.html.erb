<div class="container">
  <div class="row">

  <h1>Profiles in courage</h1>

    <% @user_profiles.each do |p| %>
    <div class="col-md-4 col-sm-4 col-xs-12 ">
      <div class="monster-card" >
       <ul>
          <li> <h1> <%= p.first_name %> </h1> </li>
          <li> <h3> <%= p.last_name %> </h3> </li>
        </ul>
      </div>
    </div>
    <% end %>

  </div>
</div>
